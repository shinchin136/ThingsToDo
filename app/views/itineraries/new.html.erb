<%= flash[:error] %>
<%= form_for(@itinerary) do |f| %>
  <fieldset>
    <div>
      <%= f.label :start_date %>
      <%= f.text_field :start_date %>
    </div>
    <div>
      <%= f.label :end_date %>
      <%= f.text_field :end_date %>
    </div>
    <div>
      <%= f.label :city %>
      <%= f.text_field :city %>
    </div>
    <div>
      <%= f.label :country %>
      <%= f.text_field :country %>
    </div>

  </fieldset>
  <%=  f.submit "Submit" %>
<% end %>